{
  "name": "svelte-material-ui-site",
  "version": "0.0.0",
  "description": "SMUI Website",
  "scripts": {
    "dev": "sapper dev -p 5000",
    "build": "sapper build --legacy",
    "export": "sapper export ./dist --legacy",
    "start": "node __sapper__/build",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark && npm run site-light && npm run site-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -s expanded -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -s expanded -i src/theme/dark",
    "site-light": "sass --no-source-map -I src/theme -I node_modules src/site.scss static/site.css",
    "site-dark": "sass --no-source-map -I src/theme/dark -I node_modules src/site.scss static/site-dark.css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
